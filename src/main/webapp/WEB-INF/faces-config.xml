<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
    http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
    


    <managed-bean>
        <managed-bean-name>input</managed-bean-name>
        <managed-bean-class>com.vsaf.common.Input</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>myBean</managed-bean-name>
        <managed-bean-class>com.vsaf.common.myBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>dbOps</managed-bean-name>
        <managed-bean-class>com.vsaf.common.DataBaseOperations</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>areaChecker</managed-bean-name>
        <managed-bean-class>com.vsaf.common.AreaChecker</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
        <managed-property>
            <property-name>input</property-name>
            <value>#{input}</value>
        </managed-property>
                <managed-property>
            <property-name>dbOps</property-name>
            <value>#{dbOps}</value>
        </managed-property>
    </managed-bean>

    
    <navigation-rule>
        <from-view-id>index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>main</from-outcome>
            <to-view-id>main.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>main.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>index</from-outcome>
            <to-view-id>index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>

